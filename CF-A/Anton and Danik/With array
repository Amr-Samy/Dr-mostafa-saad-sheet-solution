#include <bits/stdc++.h>
using namespace std;
int main()
{
    int n,a=0,d=0;
    char arr[100001];
    cin>>n;
    for(int i=0;i<n;++i)
    {
        cin>>arr[i];
        if(arr[i]=='A') {a++;}
        if(arr[i]=='D') {d++;}
    }
    if(a>d){cout<<"Anton"<<endl;}
    else if(a<d){cout<<"Danik"<<endl;}
    else if (a==d){cout<<"Friendship"<<endl;}
}
