#include <bits/stdc++.h>
using namespace std;
int main()
{
    int n,a=0,d=0;
    char x;
    cin>>n;
    for(int i=0;i<n;++i)
    {
        cin>>x;
        if(x=='A') {a++;}
        if(x=='D') {d++;}
    }
    if(a>d){cout<<"Anton"<<endl;}
    else if(a<d){cout<<"Danik"<<endl;}
    else if (a==d){cout<<"Friendship"<<endl;}
}
